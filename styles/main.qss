QPushButton { color: white;
    font-size: 14pt;
    font-family: '平方 常规';
    height: 40px;
    /* border: 1px solid #8f8f91; */
    border-radius: 6px;
    padding: 6px 18px;
    background-color: #4bd884;
}

QLabel {
    height: 40px;
    font-size: 12pt;
    font-family: '平方 常规';
    
}

QComboBox {
    height: 40px;
    font-size:12pt;
    font-family: '平方 常规';
    padding: 2px 5px;
}

#btnLoadFile {
   qproperty-icon: url(:/images/disk.png);
    qproperty-iconSize: 24px 24px;
}
#btnDownLoad {
   qproperty-icon: url(:/images/download.png);
    qproperty-iconSize: 24px 24px;
}

QPushButton:hover {
    background-color: #37a463;
}
QPushButton:pressed {
    background-color: #279352;
    border: 1px solid gray;
}
#labFileInfo {
    height: 40px;
    font-size:12pt;
    min-width: 400px;
    border: 1px solid #8f8f91;
    border-radius: 6px;
    padding: 5px;
    background-color: #fafafa;
}
#labFileInfo:hover{
    border:2px solid #37a463
}
#labFileInfo:focus {
    border:2px solid #37a463;
}
#plainTextEdit {
    font: 12pt "JetBrainsMono NF";
}